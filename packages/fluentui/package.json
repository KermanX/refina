{
  "name": "@refina/fluentui",
  "version": "0.1.0",
  "description": "The FluentUI components for Refina framework.",
  "keywords": [
    "refina",
    "fluentui",
    "components"
  ],
  "files": [
    "src",
    "dist",
    "tsconfig.json",
    "README.md"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./*": "./*"
  },
  "scripts": {
    "check": "tsc --noEmit",
    "build": "vite build",
    "dev": "vite build --watch",
    "prepublishOnly": "npm run check && npm run build"
  },
  "author": "_Kerman",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KermanX/refina"
  },
  "readme": "https://github.com/KermanX/refina#readme",
  "bugs": "https://github.com/KermanX/refina/issues",
  "license": "MIT",
  "devDependencies": {
    "@refina/tsconfig": "workspace:^",
    "typescript": "^5.2.2",
    "vite": "^4.5.2",
    "vite-plugin-refina": "workspace:^"
  },
  "dependencies": {
    "@floating-ui/dom": "^1.5.3",
    "@fluentui/keyboard-keys": "^9.0.6",
    "@fluentui/tokens": "1.0.0-alpha.9",
    "@refina/fluentui-icons": "workspace:^",
    "@refina/griffel": "workspace:^",
    "keyborg": "^2.0.0"
  },
  "peerDependencies": {
    "refina": "workspace:^"
  }
}
