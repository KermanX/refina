{
  "name": "@refina/mdui",
  "version": "0.3.0",
  "description": "The MDUI v2 components for Refina framework.",
  "keywords": [
    "refina",
    "material you",
    "mdui",
    "components"
  ],
  "files": [
    "src",
    "dist",
    "tsconfig.json",
    "README.md"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "default": "./dist/index.js",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./*": "./*"
  },
  "scripts": {
    "check": "tsc --noEmit",
    "build": "vite build",
    "dev": "vite build --watch",
    "prepublishOnly": "npm run check && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KermanX/refina"
  },
  "readme": "https://github.com/KermanX/refina#readme",
  "bugs": "https://github.com/KermanX/refina/issues",
  "license": "MIT",
  "devDependencies": {
    "@mdui/jq": "^3.0.1",
    "@mdui/shared": "^1.0.3",
    "@refina/tsconfig": "workspace:^",
    "lit": "^3.1.0",
    "typescript": "^5.2.2",
    "vite": "^4.5.2",
    "vite-plugin-refina": "workspace:^"
  },
  "dependencies": {
    "mdui": "^2.0.3"
  },
  "peerDependencies": {
    "refina": "workspace:^"
  }
}
