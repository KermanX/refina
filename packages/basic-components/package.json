{
  "name": "@refina/basic-components",
  "version": "0.3.1",
  "description": "The basic components for Refina framework.",
  "keywords": [
    "refina",
    "components"
  ],
  "files": [
    "src",
    "dist",
    "tsconfig.json",
    "README.md"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "default": "./dist/index.js",
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./*": "./*"
  },
  "scripts": {
    "check": "tsc --noEmit",
    "build": "vite build",
    "dev": "vite build --watch",
    "prepublishOnly": "npm run check && npm run build"
  },
  "author": "_Kerman",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/KermanX/refina"
  },
  "readme": "https://github.com/KermanX/refina#readme",
  "bugs": "https://github.com/KermanX/refina/issues",
  "license": "MIT",
  "devDependencies": {
    "@refina/tsconfig": "workspace:^",
    "typescript": "^5.4.2",
    "vite": "^4.5.2",
    "vite-plugin-refina": "workspace:^"
  },
  "peerDependencies": {
    "refina": "workspace:^"
  }
}
